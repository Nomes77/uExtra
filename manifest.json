{
  "name": "uExtra",
  "default_locale": "en",
  "description": "__MSG_extShortDesc__",
  "version": "0.0.5",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": {
      "128": "img/icon.png"
    },
    "default_title": "uExtra"
  },
  "options_ui": {
    "open_in_tab": true,
    "browser_style": false,
    "page": "options.html"
  },
  "permissions": [
    "storage",
    "history",
    "tabs",
    "menus"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "@uExtra",
      "strict_min_version": "91.0",
      "update_url": "https://raw.githubusercontent.com/Nomes77/uExtra/main/update.json"
    }
  },
  "commands": {
    "duplicate-tab": {
      "suggested_key": {
        "default": "Shift+Alt+D"
      },
      "description": "__MSG_toggleDuplicateTab__"
    },
    "duplicate-to-new-window": {
      "suggested_key": {
        "default": "Shift+Alt+N"
      },
      "description": "__MSG_toggleDuplicateNewWindow__"
    },
    "pop-out-to-new-incognito-window": {
      "suggested_key": {
        "default": "Shift+Alt+P"
      },
      "description": "__MSG_togglePopoutNewIncognitoWindow__"
    },
    "no-history": {
      "suggested_key": {
        "default": "Shift+Alt+Delete"
      },
      "description": "__MSG_toggleNoHistory__"
    },
    "history": {
      "suggested_key": {
        "default": "Alt+Delete"
      },
      "description": "__MSG_toggleHistory__"
    }
  },
  "icons": {
    "128": "img/icon.png"
  },
  "manifest_version": 2
}
